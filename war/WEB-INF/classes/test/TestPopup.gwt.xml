<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5.1//EN"
  "http://google-web-toolkit.googlecode.com/svn/tags/2.5.1/distro-source/core/src/gwt-module.dtd">
<module rename-to='testpopup'>
    <inherits name="com.google.gwt.user.User"/>
    <inherits name='com.google.gwt.core.Core'/>
	<inherits name='com.google.gwt.inject.Inject' />
	<inherits name='com.google.gwt.resources.Resources' />
	

	<inherits name="com.googlecode.mgwt.MGWT"/>




  <!-- Specify the app entry point class.                         -->
  <entry-point class='test.client.TestPopup'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>

	<set-property name="user.agent" value="safari" />
	<set-property name="mgwt.os" value="iphone" />  
	
	<inherits name="com.google.gwt.logging.Logging" />
	<set-property name="gwt.logging.logLevel" value="INFO" />
	<set-property name="gwt.logging.enabled" value="TRUE" />
	<set-property name="gwt.logging.popupHandler" value="DISABLED" />
	<set-property name="gwt.logging.developmentModeHandler"	value="ENABLED" />	

<!-- 	<extend-configuration-property name="gin.ginjector.extensions"  -->
<!-- 					value="buddyis.mobile.client.gin.ClientInjectorAdditional"/> -->


<!-- 	<extend-configuration-property name="gin.ginjector.modules" -->
<!-- 		     						value="buddyis.mobile.client.gin.ClientModule"/> -->

</module>
